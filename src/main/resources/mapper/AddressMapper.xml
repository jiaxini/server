<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.service.jiaxini.dao.AddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.service.jiaxini.po.Address">
        <id column="id" property="id" />
        <result column="emp_id" property="empId" />
        <result column="province" property="province" />
        <result column="city" property="city" />
        <result column="area" property="area" />
        <result column="detailed" property="detailed" />
        <result column="zip_code" property="zipCode" />
        <result column="addressee" property="addressee" />
        <result column="telphone" property="telphone" />
        <result column="is_default" property="isDefault" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, emp_id, province, city, area, detailed, zip_code, addressee, telphone, is_default, status
    </sql>

</mapper>
